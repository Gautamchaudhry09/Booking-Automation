  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.0",
    "electron-store": "^10.0.1",
    "flatpickr": "^4.6.13",
    "fs-extra": "^11.3.0",
    "mongoose": "^8.13.1",
    "os": "^0.1.2",
    "puppeteer": "^22.3.0",
    "tesseract.js": "^5.0.5"
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-packager": "^17.1.2"
  }
}
