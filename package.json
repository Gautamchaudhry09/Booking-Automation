{
  "name": "dda-sports-booking",
  "version": "1.0.0",
  "description": "DDA Sports Booking Automation",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "precaptcha": "node -e \"const fs=require('fs'); if(!fs.existsSync('./captcha')) fs.mkdirSync('./captcha');\"",
    "build": "npm run precaptcha && electron-packager . DDA-Sports-Booking --platform=win32 --arch=x64 --overwrite --extra-resource=automation.js"
  },
